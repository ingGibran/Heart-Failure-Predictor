{
    "outputDirectory": "frontend/dist",
    "functions": {
        "api/index.py": {
            "includeFiles": "api/*.joblib"
        }
    },
    "rewrites": [
        {
            "source": "/predict",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}